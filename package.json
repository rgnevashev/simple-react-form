{
  "name": "react-simpl-form",
  "version": "1.9.7",
  "license": "MIT",
  "author": "Nicolás López <nicolaslopezj@me.com>",
  "description": "A library to make reusable form components in React and React Native",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/rgnevashev/simple-react-form"
  },
  "scripts": {
    "start": "babel ./src --watch --out-dir ./lib",
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "yarn run build",
    "test": "yarn run build && yarn run lint && jest",
    "test-watch": "jest --watch",
    "lint": "standard \"src/**/*.js\""
  },
  "keywords": [
    "react-component",
    "react-native",
    "ios",
    "android",
    "form",
    "react"
  ],
  "peerDependencies": {
    "react": ">=15.3.0",
    "react-dom": ">=15.3.0"
  },
  "dependencies": {
    "dot-object": "^1.7.0",
    "lodash": "^4.17.4",
    "mongo-object": "^0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.1.0",
    "jest-cli": "^21.2.1",
    "react": "^15.6.1",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "^15.6.1",
    "standard": "^10.0.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib"
    ]
  },
  "standard": {
    "globals": [
      "it",
      "xit",
      "expect",
      "test",
      "jest",
      "MongoObject"
    ]
  }
}
